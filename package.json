{
  "name": "@study-github-agent/monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "GitHub Copilot agentを学習するためのmonorepoプロジェクト",
  "keywords": [
    "monorepo",
    "react",
    "vite",
    "typescript",
    "github-copilot"
  ],
  "scripts": {
    "app": "pnpm --filter @study-github-agent/app",
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "fmt": "turbo run fmt",
    "fmt:fix": "turbo run fmt:fix",
    "fullcheck": "turbo run lint fmt spell build test e2e typedoc",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "pre-commit": "pnpm fullcheck",
    "prepare": "husky",
    "spell": "turbo run spell",
    "test": "turbo run test",
    "e2e": "turbo run e2e",
    "type-check": "turbo run type-check",
    "typedoc": "turbo run typedoc"
  },
  "devDependencies": {
    "husky": "9.1.7",
    "turbo": "2.5.4",
    "wrangler": "4.19.1"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}
