{
  "name": "@study-github-agent/monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "GitHub Copilot agentを学習するためのmonorepoプロジェクト",
  "keywords": [
    "monorepo",
    "react",
    "vite",
    "typescript",
    "github-copilot"
  ],
  "scripts": {
    "app": "pnpm --filter @study-github-agent/app",
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "format": "turbo run format",
    "format:check": "turbo run format:check",
    "fullcheck": "turbo run lint format:check spell-check build test",
    "fullcheck:e2e": "turbo run lint format:check spell-check build test test:e2e",
    "lint": "turbo run lint",
    "pre-commit": "pnpm fullcheck",
    "prepare": "husky",
    "spell-check": "turbo run spell-check",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "type-check": "turbo run type-check",
    "typedoc": "turbo run typedoc"
  },
  "devDependencies": {
    "husky": "9.1.7",
    "sort-package-json": "3.2.1",
    "turbo": "2.5.4",
    "wrangler": "4.19.1"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}
