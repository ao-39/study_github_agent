{
  "name": "@study-github-agent/app",
  "version": "0.1.0",
  "private": true,
  "description": "Reactアプリケーション",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:analyze": "ANALYZE=true tsc && vite build",
    "clean": "rm -rf dist",
    "dev": "vite",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --max-warnings 0",
    "lint:format": "prettier --check .",
    "preview": "vite preview",
    "spell-check": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\" --no-progress --config .cspell.json --root .",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:webkit": "playwright test --project=webkit",
    "type-check": "tsc --noEmit",
    "typedoc": "typedoc src --out docs-api --exclude **/*.test.* --exclude **/*.d.ts --exclude test-setup.ts --exclude e2e/"
  },
  "prettier": "@study-github-agent/prettier-config",
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@playwright/test": "1.52.0",
    "@study-github-agent/eslint-config": "workspace:*",
    "@study-github-agent/prettier-config": "workspace:*",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "22.15.29",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "@typescript-eslint/parser": "8.33.1",
    "@vitejs/plugin-react": "4.5.1",
    "@vitest/coverage-v8": "3.2.0",
    "@vitest/ui": "3.2.0",
    "cspell": "9.0.2",
    "eslint": "9.28.0",
    "jsdom": "26.1.0",
    "prettier": "3.5.3",
    "rollup-plugin-visualizer": "6.0.1",
    "typedoc": "0.27.8",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-bundle-analyzer": "0.22.0",
    "vitest": "3.2.0",
    "yaml": "2.8.0"
  }
}
