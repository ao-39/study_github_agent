{
  "name": "@study-github-agent/app",
  "version": "0.1.0",
  "private": true,
  "description": "Reactアプリケーション",
  "type": "module",
  "scripts": {
    "build": "tsr generate && tsc && vite build",
    "build:analyze": "ANALYZE=true tsr generate && tsc && vite build",
    "clean": "rm -rf dist",
    "dev": "tsr watch --open=false & vite",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "preview": "vite preview",
    "postinstall": "pnpm exec playwright install",
    "routes:generate": "tsr generate",
    "routes:watch": "tsr watch --open=false",
    "spell": "cspell \"**/*.{ts,tsx,js,jsx,md,json}\" --no-progress --config .cspell.json --root .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "e2e": "playwright test --project=chromium",
    "e2e:chromium": "playwright test --project=chromium",
    "e2e:firefox": "playwright test --project=firefox",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "type-check": "tsc --noEmit",
    "typedoc": "typedoc src --out docs-api --exclude **/*.test.* --exclude **/*.d.ts --exclude test-setup.ts --exclude e2e/"
  },
  "prettier": "@study-github-agent/prettier-config",
  "dependencies": {
    "@tanstack/react-router": "^1.120.16",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "^3.25.55"
  },
  "devDependencies": {
    "@playwright/test": "1.52.0",
    "@study-github-agent/eslint-config": "workspace:*",
    "@study-github-agent/prettier-config": "workspace:*",
    "@tanstack/router-cli": "^1.120.16",
    "@tanstack/router-devtools": "^1.120.16",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "22.15.29",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.6",
    "@typescript-eslint/parser": "8.33.1",
    "@vitejs/plugin-react": "4.5.1",
    "@vitest/coverage-v8": "3.2.2",
    "@vitest/ui": "3.2.2",
    "cspell": "9.0.2",
    "eslint": "9.28.0",
    "jsdom": "26.1.0",
    "prettier": "3.5.3",
    "rollup-plugin-visualizer": "6.0.1",
    "typedoc": "0.28.5",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-bundle-analyzer": "0.22.0",
    "vite-plugin-pwa": "1.0.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.2",
    "yaml": "2.8.0"
  }
}
